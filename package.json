{
  "name": "web-launcher",
  "version": "2.4.1",
  "description": "A material design dashboard that replaces your new tab page and improves productivity.",
  "scripts": {
    "dev": "webpack s --env mode=prod --env platform=web --open",
    "dev:chrome": "webpack --env mode=dev --env platform=chrome --watch",
    "dev:firefox": "webpack --env mode=dev --env platform=firefox --watch",
    "build": "npm run build:web; npm run build:chrome; npm run build:firefox;",
    "build:web": "webpack --env mode=prod --env platform=web",
    "build:chrome": "webpack --env mode=prod --env platform=chrome",
    "build:firefox": "webpack --env mode=prod --env platform=firefox",
    "format": "prettier -w .",
    "lint": "eslint --fix .",
    "analyze": "webpack --env mode=prod --env platform=web --analyze"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.5.1",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-sortablejs": "^6.0.0",
    "sortablejs": "^1.13.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-extension-manifest-plugin": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "prettier": "^2.1.1",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-dev-server": "^3.11.2"
  }
}
